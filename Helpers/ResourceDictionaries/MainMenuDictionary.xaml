<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FFXIVRelicTracker.Helpers"
                    xmlns:viewmodels="clr-namespace:FFXIVRelicTracker.ViewModels"
                    xmlns:shb="clr-namespace:FFXIVRelicTracker._05_ShB.Main"
                    xmlns:skysteel="clr-namespace:FFXIVRelicTracker._05_Skysteel.Main"
                    xmlns:sb="clr-namespace:FFXIVRelicTracker._04_SB.Main"
                    xmlns:hw="clr-namespace:FFXIVRelicTracker._03_HW.Main"
                    xmlns:views="clr-namespace:FFXIVRelicTracker.Views">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/FFXIVRelicTracker;component/Helpers/ResourceDictionaries/BaseDictionary.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate  DataType="{x:Type viewmodels:MainMenuViewModel}">
        <views:MainMenuView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ArrViewModel}">
        <views:ArrView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type hw:HWViewModel}">
        <hw:HWView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type sb:SBViewModel}">
        <sb:SBView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type shb:ShBViewModel}">
        <shb:ShBView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type skysteel:SkysteelViewModel}">
        <skysteel:SkysteelView />
    </DataTemplate>

</ResourceDictionary>